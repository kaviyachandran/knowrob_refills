    <!-- http://knowrob.org/kb/shop.owl#adjacentLabelOfFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#adjacentLabelOfFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#associatedLabelOfFacing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#leftMountingFacing"/>
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#labelOfFacing"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#leftStandingFacing"/>
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#labelOfFacing"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#rightMountingFacing"/>
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#labelOfFacing"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#rightStandingFacing"/>
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#labelOfFacing"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>the label in a adjacent facing</rdfs:comment>
        <rdfs:comment>would be nice to use more general adjacentProductFacing instead</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#adjacentMountingBar -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#adjacentMountingBar">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productFacingRelation"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ShelfMountingBar"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#adjacentProductFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#adjacentProductFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productFacingRelation"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#separatorOfFacing"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://knowrob.org/kb/shop.owl#separatorOfFacing"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
        <rdfs:comment>Adjacent facings do share one separator. Each facing is adjacent to itself (due to chain axiom).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#articleNumber -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#articleNumber">
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ArticleNumber"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#articleNumberOfFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#articleNumberOfFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#articleNumber"/>
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productFacingRelation"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#preferredLabelOfFacing"/>
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#articleNumberOfLabel"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#articleNumberOfLabel -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#articleNumberOfLabel">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#articleNumber"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://knowrob.org/kb/shop.owl#ShelfLabel"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#articleNumberOfProduct -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#articleNumberOfProduct">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#articleNumber"/>
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productRelation"/>
        <rdfs:comment>an article number identitifes a product in some context.the context is determined through using dedicated datatypes shop:ean, shop:dan, etc.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#associatedLabelOfFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#associatedLabelOfFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productFacingRelation"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ShelfLabel"/>
        <rdfs:comment>relates a facing to labels that sattisfy some specific spatial constrain</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#barcodeOfProduct -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#barcodeOfProduct">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productRelation"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ShelfLabel"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#articleNumberOfProduct"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://knowrob.org/kb/shop.owl#articleNumberOfLabel"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
        <rdfs:comment>relates products to shelf labels with whom they share the ArticleNumber</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#labelOfFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#labelOfFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#associatedLabelOfFacing"/>
        <rdfs:comment>a label underneath the facing</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#layerOfFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#layerOfFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productFacingRelation"/>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://knowrob.org/kb/knowrob.owl#parts"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
        <rdfs:comment>relates a facing to the shelf layer in which it is located</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#leftMountingBar -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#leftMountingBar">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#adjacentMountingBar"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#leftMountingFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#leftMountingFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#leftProductFacing"/>
        <rdfs:domain rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacingMounting"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacingMounting"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#leftMountingBar"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://knowrob.org/kb/shop.owl#mountingBarOfFacing"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#leftProductFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#leftProductFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#adjacentProductFacing"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#leftSeparator -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#leftSeparator">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#separatorOfFacing"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#leftStandingFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#leftStandingFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#leftProductFacing"/>
        <rdfs:domain rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacingStanding"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacingStanding"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#leftSeparator"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://knowrob.org/kb/shop.owl#rightSeparator"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#mountingBarOfFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#mountingBarOfFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productFacingRelation"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ShelfMountingBar"/>
        <rdfs:comment>relates a facing to the mounting bar it contains</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#preferredLabelOfFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#preferredLabelOfFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#associatedLabelOfFacing"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#productFacingRelation -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#productFacingRelation">
        <rdfs:domain rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacing"/>
        <rdfs:comment>some abstract relation a facing has with something else</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#productInFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#productInFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productFacingRelation"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#Product"/>
        <rdfs:comment>relates a facing to a product it contains</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#productLabelOfFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#productLabelOfFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#associatedLabelOfFacing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#productInFacing"/>
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#articleNumberOfProduct"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://knowrob.org/kb/shop.owl#articleNumberOfLabel"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
        <rdfs:comment>the label of some product in the facing</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#productRelation -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#productRelation">
        <rdfs:domain rdf:resource="http://knowrob.org/kb/shop.owl#Product"/>
        <rdfs:comment>some abstract relation a product has with something else</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#rightMountingBar -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#rightMountingBar">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#adjacentMountingBar"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#rightMountingFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#rightMountingFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#rightProductFacing"/>
        <rdfs:domain rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacingMounting"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacingMounting"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#rightMountingBar"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://knowrob.org/kb/shop.owl#mountingBarOfFacing"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#rightProductFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#rightProductFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#adjacentProductFacing"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#rightSeparator -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#rightSeparator">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#separatorOfFacing"/>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#rightStandingFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#rightStandingFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#rightProductFacing"/>
        <rdfs:domain rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacingStanding"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacingStanding"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#rightSeparator"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://knowrob.org/kb/shop.owl#leftSeparator"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#separatorOfFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#separatorOfFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productFacingRelation"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ShelfSeparator"/>
        <rdfs:comment>relates facings to separators that define its width boundaries</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#siblingProductInFacing -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#siblingProductInFacing">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productRelation"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#Product"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#validFacingForProduct"/>
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#productInFacing"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>relates products to other products with the same type and which are well placed in their facing. This also includes Self if the product is in fact well-placed.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#validFacingForProduct -->

    <owl:ObjectProperty rdf:about="http://knowrob.org/kb/shop.owl#validFacingForProduct">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productRelation"/>
        <rdfs:range rdf:resource="http://knowrob.org/kb/shop.owl#ProductFacing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://knowrob.org/kb/shop.owl#articleNumberOfProduct"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://knowrob.org/kb/shop.owl#articleNumberOfFacing"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
        <rdfs:comment>yields all facings associated with the sku of some product</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#hasPhysicalComponent -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#hasPhysicalComponent"/>
    


