        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productParameter"/>
    </owl:DatatypeProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#erpFacingId -->

    <owl:DatatypeProperty rdf:about="http://knowrob.org/kb/shop.owl#erpFacingId">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#shopParameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#erpShelfId -->

    <owl:DatatypeProperty rdf:about="http://knowrob.org/kb/shop.owl#erpShelfId">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#shopParameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#erpShelfLayerId -->

    <owl:DatatypeProperty rdf:about="http://knowrob.org/kb/shop.owl#erpShelfLayerId">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#shopParameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#gtin -->

    <owl:DatatypeProperty rdf:about="http://knowrob.org/kb/shop.owl#gtin">
        <rdfs:subPropertyOf rdf:resource="http://knowrob.org/kb/shop.owl#productParameter"/>
        <rdfs:domain rdf:resource="http://knowrob.org/kb/shop.owl#ArticleNumber"/>
    </owl:DatatypeProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#hasDeviceId -->

    <owl:DatatypeProperty rdf:about="http://knowrob.org/kb/shop.owl#hasDeviceId">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasDataValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://knowrob.org/kb/shop.owl#hasUserId -->

